gid_t	T_IV
struct sockaddr_in *	T_sockaddr_in

INPUT
T_sockaddr_in
	$var = ($type)SvPV($arg,na)

OUTPUT
T_sockaddr_in
	sv_setpvn($arg, (char *)$var, sizeof(struct sockaddr_in));
